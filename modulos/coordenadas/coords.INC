#define MAX_FUELS (200)

enum fuelEnum {

	fuelLitros,
	Text3D:fuelLabel,
}
new fuelInfo[MAX_FUELS][fuelEnum];
new Iterator:fuelIter<MAX_FUELS>;

stock initFuels() {

	for(new index; index < sizeof fuelSpawns; index++) {
	    createFuel(fuelSpawns[index][0], fuelSpawns[index][1], fuelSpawns[index][2], random(10) + 10);
	    
	    // Criar galao vazio
	    new rdon = random(10);
	    
	    if (rdon < 4) {
	        createDroppedItem(fuelSpawns[index][0] + frandom(5.0) - frandom(5.0), fuelSpawns[index][1] + frandom(5.0) - frandom(5.0), fuelSpawns[index][2] + 2.0, "Galao de gasolina", ITEM_TYPE_GASOLINA, 1650, 0, true);
	    }
	}
}

stock getFuelLitros(index)
	return fuelInfo[index][fuelLitros];

stock removeFuelLitros(index, quantia) {

    fuelInfo[index][fuelLitros] -= quantia;
    
    if (fuelInfo[index][fuelLitros] < 0)
        fuelInfo[index][fuelLitros] = 0;

    new string[45];

    format(string, sizeof string, "%d%%", fuelInfo[index][fuelLitros]);
	UpdateDynamic3DTextLabelText(fuelInfo[index][fuelLabel], 0xFFFFFFF6, string);

	return 1;
}

stock getPlayerFuel(playerid) {

	foreach(new index : fuelIter) {
	
		if (IsPlayerInRangeOfPoint(playerid, 2.0, fuelSpawns[index][0], fuelSpawns[index][1], fuelSpawns[index][2])) {
		
		    return index;
		}
	}

	return -1;
}

stock createFuel(Float:x, Float:y, Float:z, litros = 100) {

	new index = Iter_Free(fuelIter);
	
	if (index != -1) {
	
	    Iter_Add(fuelIter, index);
	
	    new string[45];
	
	    fuelInfo[index][fuelLitros] = litros;
	
	    format(string, sizeof string, "%d%%", litros);
	    fuelInfo[index][fuelLabel] = CreateDynamic3DTextLabel(string, 0xFFFFFFF6, x, y, z, 5.0);
	
	    return 1;
	}
	return 0;
}

stock destroyFuel(index) {

	if (Iter_Contains(fuelIter, index)) {
	
	    Iter_Remove(fuelIter, index);
	    
	    DestroyDynamic3DTextLabel(fuelInfo[index][fuelLabel]);
	
	    return 1;
	}
	return 0;
}

