new Iterator:iterVehModelTypes[10]<615>;

stock initVehModelTypes() {

	// Iniciar modelos de veiculos
	Iter_Init(iterVehModelTypes);

	for(new types; types < sizeof (veh_ModelInfo); types ++) {
	    Iter_Add(iterVehModelTypes[veh_ModelInfo[types][modelinfo_type]], veh_ModelInfo[types][modelinfo_model]);
	}
}

stock getVehicleModelByType(type) {
	return Iter_Random(iterVehModelTypes[type]);
}

stock getMaxSlotsByModel(model) {

	for(new types; types < sizeof (veh_ModelInfo); types ++) {

		if (veh_ModelInfo[types][modelinfo_model] == model) {
	        return veh_ModelInfo[types][modelinfo_maxslots];
	    }
	}
	return 1;
}

stock getVehicleModelLootType(model) {

	for(new types; types < sizeof (veh_ModelInfo); types ++) {

		if (veh_ModelInfo[types][modelinfo_model] == model) {
	        return veh_ModelInfo[types][modelinfo_loottype];
	    }
	}
	return 1;
}
